{% extends 'base-admin.html.twig' %}

{% block body %}
    <table style=" width: 100%;float: right;">
        <tr>

            <td> <h2 style="margin-bottom: 0;padding-bottom: 0;">Arbeitszeiten</h2>
                <h3>{{objectName}}</h3>
                {% if employer != null%}
                    <h3>FÃ¼r: {{employer.firstName}} {{employer.lastName}}</h3>
                {%endif%}</td>
            <td> <img style="width:100px;height: 100px;margin-right: 0!important;right: 0;position: absolute;" src="data:image/png;base64,"></td>
        </tr>
    </table>


    <h3>{{month}}</h3><p style="font-style: italic;">Gesamt: {{ timeEntries['totalHours']}} h</p>


    <table style="width:100%;">
        <thead style="border-bottom: black solid 1px;font-weight: bold;">
        <tr>
            <td>Arbeiter</td>
            <td>Beginn</td>
            <td>Ende</td>
            <td>Gesamt</td>
        </tr>
        </thead>
        <tbody id="worktimeContent">
        {% for timeEntry in timeEntries['worktimes'] %}
            <tr>

                <td>{{ timeEntry['name'] }}</td>
                <td>{{ timeEntry['start'] | date("d.m.Y H:i") }}</td>
                <td>
                    {% if timeEntry['end'] is defined %}
                        {{ timeEntry['end'] | date("d.m.Y H:i") }}
                    {% else %}
                        -
                    {% endif %}
                </td>
                <td>{% if timeEntry['sum'] is defined %}
                        {{ timeEntry['sum'] }}
                    {% else %}
                        -
                    {% endif %}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{%endblock%}
